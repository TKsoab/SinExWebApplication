@model SinExWebApp20317887.Models.ShippingAccount

@{
    ViewBag.Title = "Account Information";
}

<h2>Account Information</h2>

@using (Html.BeginForm("GetAccount", "Account", FormMethod.Get, new { @class = "form-horizontal", role = "form" }))
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    <hr />
    if (ViewBag.AccountType == "Personal")
    {

        @Html.Partial("~/Views/PersonalShippingAccounts/Details.cshtml", Model)
    }
    else if (ViewBag.AccountType == "Business")
    {
      
        @Html.Partial("~/Views/BusinessShippingAccounts/Details.cshtml", Model)
    }

}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}

